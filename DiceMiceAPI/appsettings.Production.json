{
  "App": {
    "Urls": "${ASPNETCORE_URLS}",
    "FrontendUrl": "{FRONTEND_URL}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD};SslMode=Require"
  },
  "Jwt": {
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "EncryptionKey": "${JWT_ENCRYPTION_KEY}"
  },
  "Discord": {
    "ClientId": "${DISCORD_CLIENT_ID}",
    "ClientSecret": "${DISCORD_CLIENT_SECRET}",
    "RedirectUri": "${DISCORD_REDIRECT_URI}"
  }
}
